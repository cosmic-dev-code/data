// Tenemos un objeto con sus propiedades, cada propiedad tiene asignado un arreglo con varios elementos.
{
	// Propiedad y un arreglo como su valor.
	"nombres": [
		"Brandon",
		"Anthony",
		"Cosmic"
	],
	"Apellidos": [
		"Olivares",
		"Amador",
		"Cosmico"
	],
	// Propiedad que contiene (arrays) dentro de (arrays).
	"ajustes": [
		[true, false],
		[22, 23],
		[
			["Brandon", 22, true],
			["Anthony", 20, true],
			["Cosmic", 19, false]
		]
	],
	// Propiedad que contiene un array multidimensional.
	"trabajos": [
		["programador", "designer", "publicista"],
		["empleador", "secretario", "contador", "administrador"], 
		[
			"economizador", 
			"abogado", 
			[
				"conductor de uber",
				"conductor de autobus", 
				[
					"conductor general",
					"conductor de un campo",
					"conductor privado"
				]
			]
		]
	]
}

/* Accederiamos a las propiedades de esta manera: 
		--- nombres[0]
		
		--- ajustes[0][1]
		--- ajustes[2][0]

		--- trabajos[0]
		--- trabajos[2][2][2][1]
*/